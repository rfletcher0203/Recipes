[
   {
      "Ribs": "INGREDIENTS"
   },
   {
      "Ribs": "4 lbs pork side ribs"
   },
   {
      "Ribs": "1 1/3 cup sodium-reduced beef broth"
   },
   {
      "Ribs": "1 sweet onion finely chopped"
   },
   {
      "Ribs": "4 cloves garlic minced"
   },
   {
      "Ribs": "2 bay leaves"
   },
   {
      "Ribs": "1/4 teaspoon salt"
   },
   {
      "Ribs": "1/4 teaspoon pepper"
   },
   {
      "Ribs": "Whisky Glaze:"
   },
   {
      "Ribs": "2 cups rye whisky"
   },
   {
      "Ribs": "2 cups bourbon"
   },
   {
      "Ribs": "1/2 cup maple syrup"
   },
   {
      "Ribs": "1/4 cup packed brown sugar"
   },
   {
      "Ribs": "2 tablespoons sodium-reduced soy sauce"
   },
   {
      "Ribs": "1/4 teaspoon pepper"
   },
   {
      "Ribs": "1 pinch salt"
   },
   {
      "Ribs": "NUTRITIONAL FACTS"
   },
   {
      "Ribs": "Per each of 8 servings: about"
   },
   {
      "Ribs": "FibretraceSodium245 mgSugars18 gProtein29 gCalories415.0Total fat18 gPotassium246 mgCholesterol98 mgSaturated fat6 gTotal carbohydrate20 g%RDI"
   },
   {
      "Ribs": "Iron15.0Fibre0.0Folate2.0Sodium0.0Sugars0.0Calcium4.0Protein0.0Calories0.0Total fat0.0Vitamin A0.0Vitamin C0.0Potassium0.0Cholesterol0.0Saturated fat0.0Total carbohydrate0.0"
   },
   {
      "Ribs": "METHOD"
   },
   {
      "Ribs": "Remove membrane from underside of ribs",
      "column1": " if attached. Trim off excess fat. Cut into 2-rib portions."
   },
   {
      "Ribs": "In slow cooker",
      "column1": " combine ribs",
      "column2": " broth",
      "column3": " onion",
      "column4": " garlic",
      "column5": " bay leaves",
      "column6": " salt and pepper. Cover and cook on low until meat is tender but not falling off the bone",
      "column7": " about 5 hours. Transfer ribs"
   },
   {
      "Ribs": "to plate. (Make-ahead: Let cool for 30 minutes; refrigerate in airtight container for up to 12 hours",
      "column1": " adding about 5 minutes to grilling time.)"
   },
   {
      "Ribs": "Whisky Glaze: Meanwhile",
      "column1": " in saucepan",
      "column2": " stir together whisky",
      "column3": " maple syrup",
      "column4": " sugar",
      "column5": " soy sauce",
      "column6": " pepper and salt over medium heat; boil gently",
      "column7": " stirring occasionally",
      "column8": " until reduced and thickened enough to coat back of spoon",
      "column9": " 10 to 12 minutes."
   },
   {
      "Ribs": "Place ribs on greased grill over medium heat; close lid and grill",
      "column1": " turning and brushing with glaze",
      "column2": " until coated and sticky",
      "column3": " about 5 minutes."
   }
]
